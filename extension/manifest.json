{
    "manifest_version": 3,
    "name": "Scientific Calculator",
    "description": "Introducing my personal project, the Scientific Calculator Google Chrome extension!",
    "version": "1.0.0",
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "images/cal-logo.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
          "content.js"
        ]
      }
    ]
}